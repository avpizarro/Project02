{{> managerHeader}}
<article>
    <!-- Sidebar -->
    {{> managerNav}}
    <div class="container containerWaiter containerManager">
        <div class="row waiterRow managerRow">
            <div class="col-12 managerContentColumn">
                <table class="table empTable">
                    <thead class="thead-dark">
                        <tr>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Pin Number</th>
                            <th>Position</th>
                            <th>Manager Id</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each em}}
                        <tr>
                            <td>{{firstName}}</td>
                            <td>{{lastName}}</td>
                            <td>{{pinNumber}}</td>
                            <td>{{position}}</td>
                            <td>{{managerId}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>

        </div>
    </div>

</article>

{{> managerFooter }}