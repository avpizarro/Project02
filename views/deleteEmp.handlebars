<article id="managerPage">
    {{!-- <div class="wrapper"> --}}
    <!-- Sidebar -->
    {{> managerNav }}
        <table class="table empTable">
            <thead class="thead-dark">
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Pin Number</th>
                    <th>Position</th>
                    <th>Manager Id</th>
                </tr>
            </thead>
            <tbody>
                {{#each em}}
                    <tr>
                        <td>{{firstName}}</td>
                        <td>{{lastName}}</td>
                        <td>{{pinNumber}}</td>
                        <td>{{position}}</td>
                        <td>{{managerId}}</td>
                         <td><button class="btn btn-danger btn-sm rounded-0 deleteEmp" type="submit" title="Delete" data-id={{employeeId}}><i class="fa fa-trash"></i></button></td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
{{!-- </div> --}}
</article>

{{> managerFooter }}