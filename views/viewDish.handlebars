{{> managerHeader}}
<article class="managerArticle">
    <!-- Sidebar -->
    {{> managerNav }}
    <div class="container containerWaiter containerManager">
        <div class="row waiterRow managerRow">
            <div class="col-12 managerContentColumn">
                <table class="table empTable empForm">
                    <thead class="thead-dark">
                        <tr>
                            <th>id</th>
                            <th>Dish Name</th>
                            <th>Dish Price</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each dishes}}
                        <tr>
                            <td>{{id}}</td>
                            <td>{{title}}</td>
                            <td>$ {{price}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</article>

{{> managerFooter }}